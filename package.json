{
  "name": "@karibash/react-media-query",
  "version": "1.0.0",
  "description": "Media queries that can be used with React Hooks and CSS in JS.",
  "private": true,
  "keywords": [
    "typescript",
    "react",
    "media-query",
    "responsive",
    "breakpoint"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "jsdelivr": "dist/bundle.min.js",
  "unpkg": "dist/bundle.min.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc && tsc-alias",
    "postbuild": "npm run rollup && npm run prepare-packages && npm run prepare-dist",
    "rollup": "rollup -c tools/rollup.config.js",
    "prepare-packages": "node tools/prepare-package.js",
    "prepare-dist": "node tools/prepare-dist.js",
    "prepare": "npm run build",
    "clean": "rimraf -r dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karibash/react-media-query.git"
  },
  "author": "Karibash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Karibash/react-media-query/issues"
  },
  "homepage": "https://github.com/Karibash/react-media-query",
  "dependencies": {
    "@karibash/pixel-units": "^2.2.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/react": "^17.0.19",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "tsc-alias": "^1.3.9",
    "typescript": "^4.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
