{
  "name": "medi-q-project",
  "description": "Type-safe media queries.",
  "private": true,
  "keywords": [
    "typescript",
    "react",
    "media-query",
    "responsive",
    "breakpoint"
  ],
  "scripts": {
    "build:watch": "preconstruct watch",
    "build": "preconstruct build",
    "changeset": "changeset",
    "version-apply": "changeset version",
    "release": "yarn build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karibash/medi-q.git"
  },
  "author": "Karibash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Karibash/medi-q/issues"
  },
  "homepage": "https://github.com/Karibash/medi-q",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@changesets/changelog-github": "^0.4.1",
    "@changesets/cli": "^2.17.0",
    "@preconstruct/cli": "^2.1.2"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ],
    "globals": {
      "react": "React",
      "styled-components": "styled"
    }
  }
}
